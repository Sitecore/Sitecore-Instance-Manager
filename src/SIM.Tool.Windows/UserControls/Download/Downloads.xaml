<UserControl x:Class="SIM.Tool.Windows.UserControls.Download.Downloads" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" d:DesignHeight="205" d:DesignWidth="443" Loaded="UserControlLoaded">
	<Grid>
		<ListBox IsManipulationEnabled="False" Name="filePackages" SelectionChanged="ModuleSelected" Padding="0,5,0,0">
			<ListBox.ItemTemplate>
				<DataTemplate>
					<CheckBox Content="{Binding}" IsChecked="{Binding Path=IsChecked}" IsEnabled="{Binding Path=IsEnabled}" Margin="4 0 4 10" Name="ololo" VerticalAlignment="Center" />
				</DataTemplate>
			</ListBox.ItemTemplate>
		</ListBox>
	</Grid>
</UserControl>
